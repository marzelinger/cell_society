<?xml version="1.0" encoding="UTF-8" ?>
<data sim = "sim_test">
    <!--Simulation Name-->
    <type>RockPaperScissors</type>

    <!--Grid Parameters-->
    <rows>50</rows>
    <cols>50</cols>
    <GridShape>Hexagon</GridShape>
    <GridNeighbors>Total</GridNeighbors>
    <GridEdge>Finite</GridEdge>
    <GridOutlined>No</GridOutlined>

    <!--Actor Parameters-->
    <Threshold>0.5</Threshold>

    <!--Color Parameters-->
    <ActorColor1>YELLOW</ActorColor1>
    <ActorColor2>BLUE</ActorColor2>
    <ActorColor3>GREEN</ActorColor3>
    <EmptyColor>WHITE</EmptyColor>

    <!--Initialization Parameters-->
    <InitType>Probabilistic</InitType>
    <EmptyProbability>0.5</EmptyProbability>
    <Probability>0.3</Probability>

    <Description>This is a rock-paper-scissors simulation with a hexagonal shaped grid</Description>




</data>
